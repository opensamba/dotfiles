# DO NOT EDIT! This file will be overwritten by LXAppearance.
# Any customization should be done in ~/.gtkrc-2.0.mine instead.

gtk-theme-name="MurrinaCrystal"
gtk-icon-theme-name="LoginIcons"
gtk-font-name="Sans 9"
gtk-cursor-theme-name="DMZ-Black"
gtk-cursor-theme-size=0
gtk-toolbar-style=GTK_TOOLBAR_BOTH_HORIZ
gtk-toolbar-icon-size=GTK_ICON_SIZE_MENU
gtk-button-images=1
gtk-menu-images=1
gtk-enable-event-sounds=0
gtk-enable-input-feedback-sounds=0
gtk-xft-antialias=0
gtk-xft-hinting=1
gtk-xft-hintstyle="hintfull"
gtk-xft-rgba="rgb"
#gtk-color-scheme="tooltip_fg_color:#000000\nbase_color:#FFFFFF\nselected_fg_color:#000000000000\ntext_color:#000000\nbg_color:#dedede\ntooltip_bg_color:#F5F5C9\nselected_bg_color:#00AAFF\nfg_color:#000000\n"
gtk-color-scheme="tooltip_fg_color:#000000\nbase_color:#FFFFFF\nselected_fg_color:#00AAFF\ntext_color:#000000\nbg_color:#dedede\ntooltip_bg_color:#F5F5C9\nselected_bg_color:#00AAFF\nfg_color:#000000\n"
include "$HOME/.gtkrc-2.0.mine"
include ".gtkrc-2.0-gnome-color-chooser"
